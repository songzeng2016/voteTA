/* pages/score/score.wxss */
@import '/pages/template/template.wxss';

page {
  width: 100%;
  height: 100%;
  background: #e6e6e6;
}

.scoreWrapper {
  width: 100%;
  padding: 0 55rpx;
}

.header {
  width: 100%;
  height: 110rpx;
  line-height: 110rpx;
  padding: 0 75rpx;
  background: white;
  border-bottom: 1px solid #7c7c7c;
  color: #808080;
  font-size: 26rpx;
}

.footer {
  position: fixed;
  left: 5%;
  bottom: 36rpx;
  width: 90%;
  padding: 10rpx 0;
  background: #4aa69d;
  color: white;
  font-size: 30rpx;
}